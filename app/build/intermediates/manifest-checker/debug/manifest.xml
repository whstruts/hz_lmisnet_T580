            android:name="wcs.cps.actSupplyActive"
            android:label="@string/app_name"
            android:theme="@style/title_style" />
        <activity
            android:name="wcs.cps.actCheckStock"
            android:label="@string/app_name"
            android:theme="@style/title_style" />
        <activity
            android:name="wcs.cps.actPositionMove"
            android:label="@string/app_name"
            android:theme="@style/title_style" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="wcs.net.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>