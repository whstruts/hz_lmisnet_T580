            android:theme="@style/title_style" />
        <activity
            android:name="wcs.cps.actBlueMgr"
            android:label="@string/app_name"
            android:theme="@style/title_style" />
        <activity
            android:name="wcs.cps.actPickBoxMatch"
            android:label="@string/app_name"
            android:theme="@style/title_style" />
        <activity
            android:name="wcs.cps.actSupply"
